{
  "name": "itpm-root",
  "version": "1.0.0",
  "description": "Root project for ITPM with integrated AI Chatbot",
  "private": true,
  "scripts": {
    "dev": "cd itpm_Fe && npm run dev",
    "dev:chatbot": "cd aiChatBot/laptop-shop-chatbot && npm start",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:chatbot\"",
    "install:fe": "cd itpm_Fe && npm install",
    "install:chatbot": "cd aiChatBot/laptop-shop-chatbot && npm install",
    "install:all": "npm run install:fe && npm run install:chatbot"
  },
  "keywords": [
    "itpm",
    "chatbot",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
